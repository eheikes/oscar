{
  "name": "@eheikes/oscar-api",
  "version": "0.1.0",
  "description": "OSCAR assistant API",
  "type": "module",
  "files": [
    "dist/*",
    "LICENSE.txt"
  ],
  "scripts": {
    "prebuild": "del dist",
    "build": "tsc",
    "lint": "ts-standard --fix --verbose",
    "report-coverage": "codecov",
    "test": "NODE_OPTIONS='--loader=ts-node/esm --loader=esmock --no-warnings=ExperimentalWarning' TS_NODE_PROJECT=./tsconfig.test.json TS_NODE_FILES=true NODE_PATH=./ JASMINE_CONFIG_PATH=./test/jasmine.all.json c8 jasmine",
    "test:e2e": "NODE_OPTIONS='--loader=ts-node/esm --loader=esmock --no-warnings=ExperimentalWarning' TS_NODE_PROJECT=./tsconfig.test.json TS_NODE_FILES=true NODE_PATH=./ JASMINE_CONFIG_PATH=./test/jasmine.e2e.json jasmine \"test/e2e/**/*.spec.ts\"",
    "test:unit": "NODE_OPTIONS='--loader=ts-node/esm --loader=esmock --no-warnings=ExperimentalWarning' TS_NODE_PROJECT=./tsconfig.test.json TS_NODE_FILES=true NODE_PATH=./ JASMINE_CONFIG_PATH=./test/jasmine.unit.json c8 jasmine \"test/unit/**/*.spec.ts\"",
    "start": "node --loader ts-node/esm ./src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eheikes/oscar.git"
  },
  "keywords": [
    "api",
    "assistant",
    "productivity"
  ],
  "author": "Eric Heikes <eheikes@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/eheikes/oscar/issues"
  },
  "homepage": "https://github.com/eheikes/oscar/tree/master/packages/api#readme",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.2",
    "c8": "^9.1.0",
    "del-cli": "^5.1.0",
    "esmock": "^2.6.5",
    "jasmine": "^5.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "ts-standard": "^12.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "express": "^4.19.2",
    "serverless-http": "^3.2.0"
  },
  "standard": {
    "env": {
      "node": true,
      "jasmine": true
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": false,
    "all": false,
    "report-dir": "./coverage"
  }
}
