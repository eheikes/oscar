{
  "spec_dir": "test",
  "spec_files": [
    "acceptance/**/*.spec.ts",
    "integration/**/*.spec.ts",
    "unit/**/*.spec.ts"
  ],
  "helpers": [
    "helpers/**/*"
  ],
  "reporters": [
    {
      "name": "jasmine-spec-reporter#SpecReporter",
      "options": {
        "spec": {
          "displayPending": true,
          "displayStacktrace": "all"
        }
      }
    }
  ],
  "stopSpecOnExpectationFailure": false,
  "random": false
}
