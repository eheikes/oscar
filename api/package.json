{
  "name": "@eheikes/oscar-api",
  "version": "0.1.0",
  "description": "API server for OSCAR assistant",
  "homepage": "https://github.com/eheikes/oscar/tree/master/api",
  "repository": {
    "type": "git",
    "url": "https://github.com/eheikes/oscar.git"
  },
  "files": [
    "dist/**",
    "config.yml"
  ],
  "scripts": {
    "prebuild": "del-cli ./dist",
    "build": "tsc",
    "lint": "tslint -p tsconfig.json --fix",
    "start": "node dist/server.js",
    "test": "run-s test:*",
    "test:integration": "cross-env NODE_ENV=test JASMINE_CONFIG_PATH=test/jasmine.json jasmine-ts \"test/integration/**/*.spec.ts\"",
    "test:unit": "cross-env NODE_ENV=test JASMINE_CONFIG_PATH=test/jasmine.json nyc jasmine-ts \"test/unit/**/*.spec.ts\""
  },
  "author": "Eric Heikes <eheikes@gmail.com>",
  "license": "Apache-2.0",
  "keywords": [
    "AI",
    "assistant",
    "oscar-assistant"
  ],
  "dependencies": {
    "aws-serverless-express": "^3.3.5",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "js-yaml": "^3.12.1",
    "knex": "^0.16.3",
    "pg": "^7.9.0",
    "proxyquire": "^2.1.0"
  },
  "devDependencies": {
    "@types/aws-serverless-express": "^3.3.0",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "^3.3.7",
    "@types/js-yaml": "^3.12.0",
    "@types/knex": "^0.16.1",
    "@types/node": "^10.12.18",
    "@types/proxyquire": "^1.3.28",
    "@types/supertest": "^2.0.7",
    "@types/tempy": "^0.2.0",
    "cross-env": "^5.2.0",
    "del-cli": "^1.1.0",
    "jasmine": "^3.3.1",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "~0.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "supertest": "^3.4.1",
    "tempy": "^0.2.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.4"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".ts"
    ],
    "cache": false,
    "all": false,
    "report-dir": "./coverage"
  }
}
