{
  "name": "oscar-assistant-collector",
  "version": "0.1.0",
  "description": "Collector agent for OSCAR assistant",
  "homepage": "https://github.com/eheikes/oscar/collector",
  "repository": {
    "type": "git",
    "url": "https://github.com/eheikes/oscar.git"
  },
  "bin": {
    "oscar-collector": "./build/src/main.js"
  },
  "scripts": {
    "build": "tsc && chmod a+x ./build/src/main.js",
    "clean": "del-cli ./coverage ./build",
    "dev": "watch \"run-s clean build test\" ../types src test",
    "lint": "tslint -t stylish --project . --fix",
    "start": "node build/src/main.js",
    "test": "cross-env JASMINE_CONFIG_PATH=test/jasmine.json nyc jasmine-ts"
  },
  "author": "Eric Heikes <eheikes@gmail.com>",
  "license": "Apache-2.0",
  "keywords": [
    "AI",
    "assistant",
    "oscar-assistant"
  ],
  "dependencies": {
    "caminte": "^0.4.0",
    "cross-env": "^5.1.3",
    "del-cli": "^1.1.0",
    "feedparser": "^2.2.7",
    "js-yaml": "^3.10.0",
    "mysql": "^2.15.0",
    "pg": "^7.4.1",
    "pkg-dir": "^2.0.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "sqlite3": "^3.1.13",
    "uuid": "^3.1.0",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "@types/feedparser": "^2.2.1",
    "@types/jasmine": "^2.8.2",
    "@types/js-yaml": "^3.10.1",
    "@types/node": "^8.0.57",
    "@types/pkg-dir": "^2.0.0",
    "@types/request": "^2.0.9",
    "@types/request-promise-native": "^1.0.10",
    "@types/tempy": "^0.1.0",
    "@types/uuid": "^3.4.3",
    "@types/yargs": "^10.0.0",
    "jasmine": "^2.8.0",
    "jasmine-ts": "^0.2.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.3.0",
    "tempy": "^0.2.1",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.2",
    "watch": "^1.0.2"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".ts"
    ],
    "cache": false,
    "all": false,
    "report-dir": "./coverage"
  }
}
